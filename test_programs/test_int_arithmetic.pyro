func int_adding(integer a, integer b) > integer {
    return a + b;
}

func int_subtract(integer a, integer b) > integer {
    return a - b;
}

func int_multiply(integer a, integer b) > integer {
    return a * b;
}

func int_divide(integer a, integer b) > integer {
    return a / b;
}

func main() {
    let a = 3;
    let b = 4;
    print("{int} + {int} = {int}", a, b, int_adding(a, b));
    print("{int} - {int} = {int}", a, b, int_subtract(a, b));
    print("{int} * {int} = {int}", a, b, int_multiply(a, b));
    print("{int} / {int} = {int}", a, b, int_divide(a, b));
    print("{int} / {int} = {int}", 8, 4, 8 / 4);
}